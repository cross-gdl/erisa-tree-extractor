{
  "manifest_version": 3,
  "name": "ERISA Tree Extractor",
  "version": "1.0",
  "description": "Expands and extracts Fancytree folder structures to clipboard",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}
